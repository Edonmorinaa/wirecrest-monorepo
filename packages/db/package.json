{
  "name": "@wirecrest/db",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@prisma/client": "6.18.0"
  },
  "devDependencies": {
    "@wirecrest/typescript-config": "workspace:*",
    "prisma": "6.18.0"
  },
  "scripts": {
    "generate": "prisma generate --schema=./prisma/schema.prisma",
    "build": "prisma generate --schema=./prisma/schema.prisma && npx tsc --project tsconfig.json",
    "dev": "echo 'No dev step required for db package'",
    "lint": "echo 'No lint step required for db package'",
    "clean": "rm -rf dist",
    "type-check": "npx tsc --noEmit",
    "seed:feature-flags": "tsx seed/seedFeatureFlags.ts"
  }
}
