{
  "name": "@wirecrest/scraper",
  "version": "1.0.0",
  "description": "Production-ready Google Review Analytics Service with comprehensive error handling, monitoring, and performance optimization",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --skipLibCheck",
    "postbuild": "bash scripts/validate-build.sh 2>/dev/null || echo 'Build validation skipped'",
    "start": "node dist/server.js",
    "start:old": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:old": "ts-node-dev --respawn --transpile-only src/index.ts",
    "demo:actor-manager": "ts-node src/apifyService/actorManagerExample.ts",
    "test:workflow": "node test-workflow.js",
    "test:production": "node dist/test-production-system.js",
    "test:lamatok": "ts-node scripts/test-lamatok-api.ts",
    "mock:instagram": "ts-node scripts/run-mock-data.ts",
    "mock:6months": "ts-node scripts/run-6-months-mock-data.ts",
    "mock:instagram-only": "ts-node scripts/insert-mock-instagram-data.ts",
    "mock:tiktok-only": "ts-node scripts/insert-mock-tiktok-data.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "docker:build": "docker build -t wirecrest-review-api .",
    "docker:run": "docker run -p 3000:3000 --env-file .env wirecrest-review-api",
    "health:check": "curl -f http://localhost:3000/health || exit 1"
  },
  "dependencies": {
    "@nlpjs/sentiment": "^5.0.0-alpha.5",
    "@supabase/supabase-js": "^2.45.4",
    "@types/pg": "^8.15.2",
    "@wirecrest/auth-core": "workspace:^",
    "@wirecrest/billing": "workspace:^",
    "@wirecrest/db": "workspace:^",
    "@wirecrest/notifications": "workspace:^",
    "apify-client": "^2.12.2",
    "axios": "^1.6.0",
    "bullmq": "^2.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "node-nlp": "^5.0.0-alpha.5",
    "pg": "^8.16.0",
    "prom-client": "^15.1.0",
    "stripe": "^14.12.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@wirecrest/typescript-config": "workspace:*",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "google-reviews",
    "analytics",
    "sentiment-analysis",
    "supabase",
    "express",
    "production-ready"
  ],
  "author": "Wirecrest Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wirecrest/review-analytics-api"
  }
}
